<!DOCTYPE html>
<html>
  <head>
    <title>MyPets</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script src="https://kit.fontawesome.com/1d8af2b292.js" crossorigin="anonymous"></script><!--fontawesome機能-->
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/><!--slick機能-->
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css"/>
  </head>

  <body>
    <!--<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js"></script>-->
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script><!--slick機能-->

    <%= render 'layouts/header' %>

    <main>
      <% if flash[:notice] %>
        <div class="flash mt-2"><%= flash[:notice] %></div>
      <% end %>
    <%= yield %>
    </main>
    <%= render 'layouts/footer' %>
  </body>
</html>
